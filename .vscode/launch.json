  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch mern",
      "protocol": "inspector",
      "runtimeExecutable": "nodemon",
      "runtimeArgs": [
          "--inspect=3001"
      ],
      "program": "${workspaceFolder}/server.js",
      "port": 3001,
      "restart": true,
      "env": {
          "BABEL_DISABLE_CACHE": "1",
          "NODE_ENV": "development"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },{
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}\\server.js"
    }
  ]
}